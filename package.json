{
  "name": "nodelab",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "start": "node index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc && nodemon dist/index.js",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint . --fix --ext .ts"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "testEnvironment": "node",
    "rootDir": "./src",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts)": [
      "npm run lint",
      "git add"
    ]
  },
  "author": "Anna Celina",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.2.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.2.6",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.12",
    "node-cron": "^3.0.0",
    "pg": "^8.7.1",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.3",
    "typeorm": "^0.2.40",
    "winston": "^3.3.3",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node-cron": "^3.0.1",
    "@types/pg": "^8.6.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
